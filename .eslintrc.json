{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style":"off",
        "no-undef":"off",
        "no-underscore-dangle":"off",
        "class-methods-use-this": "off",
        "no-unused-vars":"off"
    }
}
